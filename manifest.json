{
  "manifest_version": 3,
  "name": "Page Price Analyzer - Job Number Extractor",
  "version": "1.0.0",
  "description": "Extract job numbers from web pages and submit them to your database. Perfect for job tracking and analysis.",
          "permissions": [
          "activeTab",
          "storage",
          "scripting",
          "identity"
        ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Page Price Analyzer - Job Number Extractor"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Your Name",
  "homepage_url": "https://pricerr.vercel.app/",
  "short_name": "Job Extractor"
}
