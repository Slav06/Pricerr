{
  "manifest_version": 3,
  "name": "Page Price Analyzer",
  "version": "1.0.0",
  "description": "Extract job numbers from web pages",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.hellomoving.com/*",
        "*://ant.hellomoving.com/*"
      ],
      "js": ["content.js"]
    }
  ],
      "web_accessible_resources": [
        {
          "resources": ["payment-service.js", "payment-config.js", "payment-encryption.js", "payment-fields-test.html", "payment-test-script.js"],
          "matches": ["*://*.hellomoving.com/*", "*://ant.hellomoving.com/*"]
        }
      ],
  "background": {
    "service_worker": "background.js"
  }
}
